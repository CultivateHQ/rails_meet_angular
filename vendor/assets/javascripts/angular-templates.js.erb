var AngularTemplates = (function(){

  var loadTemplates = function($templateCache) {
    <% RailsMeetAngular::Helpers.templates(self) do |path, data| %>
      $templateCache.put('/assets/<%= path %>', '<%= data %>');
    <% end %>
  }

  loadTemplates.$inject = ['$templateCache']

  return {load: loadTemplates}
})()
